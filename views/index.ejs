<body>
    <div class="mainbox">
    <div class="newpages">
        <div class="header">
            <a href=""><h1 class="aurjobs">Aurjobs</h1></a>

            <input type="checkbox" class="sidemenu" name="sidemenu" id="sidemenu">
            <label for="sidemenu" class="newmenu"><span class="sidemenuline"></span></label>

            <div class="menu">
             <svg class="line-top" width="750" height="15" viewbox="0,0 1000,20">
              <line
               class="line-dash"
               x1="0"
               y1="15"
               x2="1000"
               y2="15"
               style="stroke: orange; stroke-width:2; stroke-linecap:round; stroke-dasharray: 180,1200; stroke-dashoffset: -35;"
              />
             </svg>
             <ul>
                <li>Home</li><a href="addpages.html"><li>Blogs</li></a><a href="subscriptionai.html"><li class="subscribe"><img src="heading.png" alt=""></li></a><a><li><form action="/newjobpostpage"><input type="submit" value="Hire"></form></li></a><a href="about.html"><li>About</li></a>
             </ul>
             <svg class="line-bottom" width="750" height="30" viewbox="0,0 1000,40">
              <polygon class="lb" points="35,53 115,53 125,43 135,53 215,53" />
              <polygon class="lb" points="285,53 365,53 375,43 385,53 465,53" />
           
              <polygon class="lb" points="535,53 615,53 625,43 635,53 715,53" />
           
              <polygon class="lb" points="785,53 865,53 875,43 885,53 965,53" />
             </svg>
            </div>
           </div>

           <!-- new menu -->
           <div class="newmenu">
           <div id="hamburger-icon" onclick="toggleMobileMenu(this)">
            <div class="bar1"></div>
            <div class="bar2"></div>
            <div class="bar3"></div>
            <ul class="mobile-menu">
              <li><a><form action="aurjobs10.onrender.com">Home</form></a></li>
              <li><form action="/blogs"><input type="submit" value="Blogs"></form></li>
              <li><form action="/subscribe"><input class="newmenusubscribeai" type="submit" value="Subscription.ai"></form></li>
              <li id="login"><form action="/newjobpostpage"><input type="submit" value="Hire"></form></li>
              <li id="signup"><a href="about.html">about</a></li>
            </ul>
          </div>
          <h1 class="newheading"><img class="newheadingimage" src="aurjobs.png" alt=""></h1>
          </div>
    

           <div class="companies">
            <p class="attribution"><a href="aurjobs10.onrender.com">@Aurjobs</a> Helps You Search the right Jobs in MNCs. Founded by <a href="https://www.linkedin.com/in/keshav-agarwal-805757220">@Keshav Agarwal</a>. Provides you Checked Jobs from Big Companies like <a href="">@Google</a>,<a href="">@Apple</a>. Search Jobs from Various Domains IT, Banking. Get your Job Now.</p>
            <div class="wrapper">
                <div class="gallery">
                    <ul>
                        <li><img src="companies/Google.png" alt="google"></li>
                        <li><img src="companies/facebook.png" alt="meta"></li>
                        <li><img src="companies/amazon.png" alt="amazon"></li>
                        <li><img src="companies/microsoft.png" alt="microsoft"></li>
                        <li><img src="companies/apple.png" alt="apple"></li>
                        <li><img src="companies/netflix.png" alt="netflix"></li>
                        <li><img src="companies/Deloitte.png"></li>
                        <li><img src="companies/aurjobs-high-resolution-logo-transparent.png"></li>
                        <li><img src="companies/atlassian.png"></li>
                    </ul>
                </div>
            </div>
           </div>

    </div>
    <form action="/search">
    <section class="search">
        <div class="searchmain">
        <div class="searchbox">
        <input type="text" name="search" class="searchtext" id="search" placeholder="Search">
        <button type="submit">Find</button>
        </div>
        </div>
    </section>
    <section class="jobs">

    <section class="jobsfilter">
        <div class="filterbox">
        <h2>job Category</h2>
        <input type="text" name="category" class="category" id="category">
        </div>
        <div class="filterbox">
        <h2>job location</h2>
        <input type="text" name="location" class="location" id="location">
        </div>
        <div class="filterbox">
        <h2>job type</h2>
        <input type="text" name="jobtype" class="jobtype" id="jobtype">
        </div>
    </section>
    </form>
    <section class="jobslist">
            <% data.forEach(sample => { %>
        <div class="itemcard">
            <div class="itemimg">
                <!-- img <%= sample.sno %> -->
                <img src=<%= sample.imglink %> alt="Aurjobs">
            </div>
            <div class="job">
            <h1 class="title">
                <%= sample.title %>
            </h1>
            <h2 class="jobcompany">
                <%= sample.company %>
            </h2>
            <p class="jobdesc">
                <ul>
                    <li class="newjobdesc"><img src="job-description.png" alt="" class="descimg"> <%= sample.category %> </li>
                    <li class="newjobdesc"><img src="location.png" alt="location" class="descimg"> <%= sample.location %> </li>
                    <li class="newjobdesc"><img src="application.png" alt="jobtype" class="descimg"> <%= sample.jobtype %> </li>
                </ul>
                <!-- <%= sample.desc %> -->
            </p>
            </div>
            <!-- <%= sample.link %> -->
            <form action="/itempage">
            <!-- <div class="joblink" id="joblink"> -->
            <input type="hidden" name="joblink" class="joblink" id="joblink" value=<%= sample.link %>>
            <!-- <h4 class="joblink" id="joblink"><%= sample.link %></h4> -->
            <!-- </div> -->
            <button type="submit">Apply</button>
            </form>
        </div>
        <% }) %>
    </section>
    
    </section>
    </div>

    <section class="bottom">
        <div class="newbottomcontact">
            <h1>Address</h1>
            <h4>New Delhi, India</h4>
            <p><h4><a href="mailto:aurjobsa@gmail.com">Aurjobsa@gmail.com</a></h4></p>
            <p><h4>Phone</h4></p>
        </div>
        <div class="newbottomlinks">
            <h1>Latest</h1>
            <div class="newlinks">
            <div><a href="https://www.linkedin.com/company/aurjobs"><img src="linkedin.svg" alt=""></a></div>
            <div><a href="https://www.instagram.com/aurjobsofficial/"><img src="instagram.svg" alt=""></a></div>
            <div><a href="https://www.facebook.com/people/AurjobsOfficial/61556103504038"><img src="facebook.svg" alt=""></a></div>
            <div><a href="https://twitter.com/ChaubeyAnshika_"><img src="twitter.svg" alt=""></a></div>
            </div>
            <p>Contact with us To stay Updated</p>
        </div>
        <div class="newbottompages">
            <h1>Pages</h1>
            <p><h4>Home</h4></p>
            <p><h4>Blogs</h4></p>
            <p><h4>Resume Review</h4></p>
            <p><h4>Subscription.ai</h4></p>
            <p><h4>About</h4></p>
        </div>
    </section>

